{
  "name": "province-city-china",
  "version": "8.5.8",
  "description": "An util to query china province, city and district data. 中华人民共和国国家标准 GB/T 2260 行政区划代码。",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "①": "------------获取<省市区>数据---------------------------",
    "get": "node script/province.js",
    "②": "------------获取香港澳门<区>数据------------------------------",
    "hongkong": "node script/hongkong-macau.js",
    "③": "------------检查<区>数据------------------------------",
    "check": "node script/check.js",
    "④": "------------获取<街道>数据-----------------------------",
    "get:town": "node script/town.js",
    "⑤": "------------生成<SQL><省市区(data.json)>数据-----------",
    "sql": "node script/sql.js",
    "⑥": "------------重新生成新的<层级>数据----------------------",
    "level": "node script/level.js",
    "⑦": "------------获取<长途电话区号>数据------------------------",
    "code": "node script/district-code.js",
    "⑧": "------------数据拷贝到各个<包>中------------------------",
    "copy": "node script/copy.js"
  },
  "keywords": [
    "province",
    "china",
    "cn",
    "city",
    "GB",
    "GB2260",
    "中国",
    "中华人民共和国",
    "行政区划",
    "省市",
    "省市区",
    "联动",
    "省份",
    "城市",
    "县市",
    "区县",
    "乡镇",
    "街道",
    "村委会",
    "居委会",
    "省级",
    "地级",
    "县级",
    "乡级",
    "村级"
  ],
  "files": [
    "dist",
    "data.js",
    "index.d.ts",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/province-city-china.git"
  },
  "license": "MIT",
  "dependencies": {
    "@province-city-china/types": "8.5.8"
  },
  "devDependencies": {
    "cheerio": "~1.0.0-rc.10",
    "csvtojson": "~2.0.10",
